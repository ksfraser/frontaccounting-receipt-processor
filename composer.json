{
    "name": "frontaccounting/receipt-processor",
    "description": "A receipt processing application for Front Accounting",
    "type": "project",
    "require": {
        "monolog/monolog": "^2.0",
        "symfony/http-foundation": "^5.0",
        "symfony/finder": "^7.3",
        "symfony/filesystem": "^7.3",
        "thiagoalessio/tesseract_ocr": "^2.13"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "App\\Utils\\Errors\\": "src/Utils/Errors/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
